    "express": "^4.17.1",
    "fast-glob": "^3.2.5",
    "googleapis": "^87.0.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.0",
    "multer": "^1.4.3",
    "multer-s3": "^2.9.0",
    "node-fetch": "^2.6.5",
